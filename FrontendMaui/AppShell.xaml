<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="FrontendMaui.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:FrontendMaui"
    Shell.FlyoutBehavior="Flyout"
    Title="FrontendMaui">

    <FlyoutItem Title="Neptun manager" Route="Main">
        <ShellContent Route="Login" ContentTemplate="{DataTemplate local:LoginPage}" />
        <ShellContent Route="MainPage" ContentTemplate="{DataTemplate local:MainPage}" />
        <ShellContent Route="addnewstudent" ContentTemplate="{DataTemplate local:AddNewStudentPage}"/>
    </FlyoutItem>

</Shell>

